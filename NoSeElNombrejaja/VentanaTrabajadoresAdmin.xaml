<UserControl x:Class="NoSeElNombrejaja.VentanaTrabajadoresAdmin"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:NoSeElNombrejaja"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>
        <DataGrid Margin="20" Name="Lista" Background="{x:Null}" Foreground="Black" FontWeight="Bold" FontFamily="Cambria" FontSize="17">
            
        </DataGrid>
        <Border Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center" Background="#FF36281F" BorderBrush="#FF231F20" 
                    BorderThickness="3" Margin="15" Name="BtnCrearAnuncio">
            <Label FontFamily="cambria" FontSize="30" FontWeight="Bold" Foreground="AntiqueWhite" MouseLeftButtonUp="BtnAñadirUsuario" >
                añadir Usuario
            </Label>
        </Border>
    </Grid>
</UserControl>
